{
  "name": "github-contribution-api",
  "description": "Scrape contribution data of a Github user",
  "version": "4.6.3",
  "author": "Jonathan Gruber <gruberjonathan@gmail.com>",
  "license": "MIT",
  "private": true,
  "main": "build/server.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "rm -rf build/* && tsc",
    "dev": "nodemon src/server.ts --exec ts-node src/server.ts",
    "format": "prettier --write src tests",
    "prepare": "husky install",
    "postinstall": "npm run build",
    "prestart": "npm run build",
    "start": "node build/server.js",
    "test": "jest",
    "typecheck": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grubersjoe/github-contributions-api.git"
  },
  "bugs": {
    "url": "git+https://github.com/grubersjoe/github-contributions-api.git"
  },
  "keywords": [
    "github",
    "contributions",
    "api",
    "node"
  ],
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cors": "^2.8.4",
    "express": "^4.18.2",
    "memory-cache": "^0.2.0",
    "rotating-file-stream": "^3.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "^7.22.5",
    "@types/cheerio": "^0.22.35",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/memory-cache": "^0.2.3",
    "@types/node": "^20.10.4",
    "@types/supertest": "^2.0.12",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
